						<groupId>
							org.apache.maven.plugins
							</groupId>
						<artifactId>
							maven-assembly-plugin
							</artifactId>
						<version>
							3.1.1
							</version>
						<executions>
							<execution>
								<phase>
									package
									</phase>
								<goals>
