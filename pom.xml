<?xml version = "1.0" encoding = "UTF-8" ?>
<project
		xmlns = "http://maven.apache.org/POM/4.0.0"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>
		4.0.0
	</modelVersion>
	<groupId>
		ws.nzen.format.eno
	</groupId>
	<artifactId>
		enojar
	</artifactId>
	<version>
		0.0
	</version>
	<inceptionYear>
		2018
	</inceptionYear>
	<licenses>
		<license>
			<name>
				MIT v1.0
			</name>
			<url>
				https://mitlicense.org
			</url>
		</license>
	</licenses>
	<developers>
		<developer>
			<id>
				nzen
			</id>
			<name>
				Nicholas Prado
			</name>
			<email>
				nmprado@nzen.ws
			</email>
			<roles>
				<role>
					Implementor
				</role>
			</roles>
			<timezone>
				UTC-06:00
			</timezone>
		</developer>
	</developers>
	<dependencies>
		<!--
		<dependency>
			<groupId>
				
			</groupId>
			<artifactId>
				
			</artifactId>
			<version>
				
			</version>
		</dependency>
		-->
		<dependency>
			<groupId>
				org.junit
			</groupId>
			<artifactId>
				junit-bom
				<!-- this time, I'll just ask for everything -->
			</artifactId>
			<version>
				5.3.1
			</version>
			<scope>
				test
			</scope>
			<type>
				pom
			</type>
		</dependency>
	</dependencies>
	<build>
	    <plugins>
	    	<!-- make source jar -->
		    <plugin>
				<groupId>
					org.apache.maven.plugins
				</groupId>
				<artifactId>
					maven-source-plugin
				</artifactId>
				<executions>
					<execution>
						<id>
							attach-sources
						</id>
						<goals>
						<goal>
							jar
						</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
	    	<!-- skip tests without remembering -DskipTests -->
			<plugin>
				<groupId>
					org.apache.maven.plugins
				</groupId>
				<artifactId>
					maven-surefire-plugin
				</artifactId>
				<version>
					2.12.4
				</version>
				<configuration>
					<skipTests>
						true
					</skipTests>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<properties>
	    <!-- jdk v3 is the maven default -->
		<maven.compiler.source>
			1.10
		</maven.compiler.source>
		<maven.compiler.target>
			1.10
		</maven.compiler.target>
	</properties>
	<description>
		An eno-document serialization library. Review eno at http://eno-lang.org
	</description>
	<name>
		Enojar
	</name>
</project>
